{
  "name": "service-one",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Mateus Zucco",
  "license": "MIT",
  "config": {
    "PROJECT_ID": "standardprototype",
    "SERVICE_NAME": "service-one",
    "REGION": "us-central1"
  },
  "scripts": {
    "build": "rimraf ./build && tsc",
    "dev": "nodemon",
    "start": "node build/index.js",
    "clean": "tsc --build --clean",
    "gcp-build": "tsc",
    "cloud:build": "gcloud builds submit --tag %npm_package_config_REGION%-docker.pkg.dev/%npm_package_config_PROJECT_ID%/cloud-run-source-deploy/%npm_package_config_SERVICE_NAME% --project %npm_package_config_PROJECT_ID%",
    "cloud:deploy": "gcloud run deploy %npm_package_config_SERVICE_NAME% --image %npm_package_config_REGION%-docker.pkg.dev/%npm_package_config_PROJECT_ID%/cloud-run-source-deploy/%npm_package_config_SERVICE_NAME% --region %npm_package_config_REGION% --allow-unauthenticated --memory 128Mi --port 8080 --set-env-vars DB_SOCKET_PATH=,DB_HOST=,DB_PORT=,DB_USER=,DB_PASS=,DB_NAME= ",
    "deploy": "yarn cloud:build && yarn cloud:deploy"
  },
  "keywords": [],
  "dependencies": {
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.14.1"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/morgan": "^1.9.9",
    "@types/mysql": "^2.15.27",
    "@types/node": "^22.15.17",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}

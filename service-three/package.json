{
  "name": "service-three",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Mateus Zucco",
  "license": "MIT",
  "config": {
    "PROJECT_ID": "standardprototype",
    "SERVICE_NAME": "service-three-grpc",
    "REGION": "us-central1"
  },
  "scripts": {
    "build": "rimraf ./build && tsc && yarn copy-assets",
    "copy-assets": "copyfiles -u 1 src/**/*.proto dist/",
    "dev": "nodemon",
    "start": "node build/index.js",
    "clean": "tsc --build --clean",
    "gcp-build": "tsc && yarn copy-assets",
    "cloud:build": "gcloud builds submit --tag %npm_package_config_REGION%-docker.pkg.dev/%npm_package_config_PROJECT_ID%/cloud-run-source-deploy/%npm_package_config_SERVICE_NAME% --project %npm_package_config_PROJECT_ID%",
    "cloud:deploy": "gcloud run deploy %npm_package_config_SERVICE_NAME% --image %npm_package_config_REGION%-docker.pkg.dev/%npm_package_config_PROJECT_ID%/cloud-run-source-deploy/%npm_package_config_SERVICE_NAME% --region %npm_package_config_REGION% --allow-unauthenticated --max-instances 1 --min-instances 0 --timeout=3600 --concurrency 12 --cpu 1 --memory 128Mi --use-http2 --port 3003",
    "deploy": "yarn cloud:build && yarn cloud:deploy"
  },
  "keywords": [],
  "dependencies": {
    "@grpc/grpc-js": "^1.14.0",
    "@grpc/proto-loader": "^0.8.0",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.14.1"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/morgan": "^1.9.9",
    "@types/mysql": "^2.15.27",
    "@types/node": "^24.5.2",
    "copyfiles": "^2.4.1",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
